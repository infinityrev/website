//Rewrite to www
Options -Indexes 
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^prashanta.gq[nc]
RewriteRule ^(.*)$ http://prashanta.gq/$1 [r=301,nc]

//Custom 400 errors
ErrorDocument 400 /404/index.html

//Custom 401 errors
ErrorDocument 401 /404/index.html

//Custom 403 errors
ErrorDocument 403 /404/index.html

//Custom 404 errors
ErrorDocument 404 /404/index.html

//Custom 500 errors
ErrorDocument 500 /404/index.html
 